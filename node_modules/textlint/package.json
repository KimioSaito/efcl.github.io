{
  "name": "textlint",
  "description": "The pluggable linting tool for text and markdown.",
  "version": "5.2.0",
  "homepage": "https://github.com/textlint/textlint/",
  "keywords": [
    "plugable",
    "text",
    "textlint",
    "markdown",
    "lint",
    "linting",
    "AST"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/textlint/textlint.git"
  },
  "main": "lib/index.js",
  "files": [
    "bin",
    "lib",
    "src"
  ],
  "bin": {
    "textlint": "./bin/textlint.js"
  },
  "scripts": {
    "build": "babel src --out-dir lib --source-maps",
    "watch": "babel src --out-dir lib --watch --source-maps",
    "prepublish": "npm run --if-present build",
    "lint:doc": "eslint docs/ --ext md",
    "lint:js": "jscs src",
    "test:example": "./test/example-test.sh",
    "test": "mocha test/*.js && npm run lint:doc && npm run lint:js && npm run test:example",
    "perf": "cd perf && npm run perf"
  },
  "directories": {
    "test": "test/"
  },
  "author": {
    "name": "azu"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/textlint/textlint/issues"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "clone": "^1.0.0",
    "eslint": "^1.10.3",
    "eslint-plugin-markdown": "github:eslint/eslint-plugin-markdown",
    "espower-babel": "^3.3.0",
    "glob": "^6.0.1",
    "jscs": "^2.1.0",
    "mocha": "^2.3.3",
    "power-assert": "^1.0.1",
    "textlint-plugin-html": "^0.1.2",
    "textlint-rule-helper": "^1.1.3",
    "textlint-rule-no-todo": "^1.0.2"
  },
  "dependencies": {
    "bluebird": "^3.0.5",
    "carrack": "0.0.4",
    "concat-stream": "^1.4.7",
    "debug": "^2.1.0",
    "interop-require": "^1.0.0",
    "mkdirp": "^0.5.0",
    "object-assign": "^4.0.1",
    "optionator": "^0.8.0",
    "rc-loader": "^1.0.0",
    "textlint-formatter": "^1.5.0",
    "textlint-plugin-markdown": "^1.0.1",
    "textlint-plugin-text": "^1.0.1",
    "traverse": "^0.6.6",
    "try-resolve": "^1.0.1",
    "txt-ast-traverse": "^1.2.0",
    "unique-concat": "^0.2.2"
  },
  "gitHead": "68bc1610af4c3b2a23bfb59c85b9623a3fadbe2f",
  "_id": "textlint@5.2.0",
